	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\ZE3ZRXNK\\4020363.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\JUWZBUSI\\Jones 和 Walker - 2006 - Practical Kinematics for Real-Time Implementation .pdf:application/pdf}
