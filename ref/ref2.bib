	pages = {5067--5074},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\VPD7DHNW\\7139904.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\X3E8K5N7\\Till 等. - 2015 - Efficient computation of multiple coupled Cosserat.pdf:application/pdf}
