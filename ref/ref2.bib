	pages = {5919--5926},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\43FQIUZI\\5648888.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\A4J4TGPE\\Xiao 和 Vatcha - 2010 - Real-time adaptive motion planning for a continuum.pdf:application/pdf}
}

@inproceedings{segreti_cable_2012,
