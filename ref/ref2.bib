
@inproceedings{butler_robotic_2012,
	title = {Robotic neuro-emdoscope with concentric tube augmentation},
	doi = {10.1109/IROS.2012.6386022},
	abstract = {Surgical robots are gaining favor in part due to their capacity to reach remote locations within the body. Continuum robots are especially well suited for accessing deep spaces such as cerebral ventricles within the brain. Due to the entry point constraints and complicated structure, current techniques do not allow surgeons to access the full volume of the ventricles. The ability to access the ventricles with a dexterous robot would have significant clinical implications. This paper presents a concentric tube manipulator mated to a robotically controlled flexible endoscope. The device adds three degrees of freedom to the standard neuroendoscope and roboticizes the entire package allowing the operator to conveniently manipulate the device. To demonstrate the improved functionality, we use an in-silica virtual model as well as an ex-vivo anatomic model of a patient with a treatable form of hydrocephalus. In these experiments we demonstrate that the augmented and roboticized endoscope can efficiently reach critical regions that a manual scope cannot.},
	booktitle = {2012 {IEEE}/{RSJ} {International} {Conference} on {Intelligent} {Robots} and {Systems}},
	author = {Butler, E. J. and Hammond-Oakley, R. and Chawarski, S. and Gosline, A. H. and Codd, P. and Anor, T. and Madsen, J. R. and Dupont, P. E. and Lock, J.},
	month = oct,
	year = {2012},
	keywords = {Actuators, concentric tube manipulator, continuum robots, entry point constraints, surgical robots, in-silica virtual model, Wires, endoscopes, dexterous robot, Electron tubes, ex-vivo anatomic model, remote locations, concentric tube augmentation, robotic neuro-endoscope, Medical robotics, hydrocephalus, patient treatment, surgery, dexterous manipulators, Manipulators},
	pages = {2941--2946},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\76CWTFNG\\6386022.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\KVUF6NZJ\\Butler 等. - 2012 - Robotic neuro-emdoscope with concentric tube augme.pdf:application/pdf}
}

