	pages = {785--792},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\SNUEGRVH\\5766760.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\VJIR5NDN\\Kanoun 等. - 2011 - Kinematic Control of Redundant Manipulators Gener.pdf:application/pdf}
