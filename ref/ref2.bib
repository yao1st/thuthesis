	author = {Wu, L. and Crawford, R. and Roberts, J.},
	month = apr,
	year = {2017},
	keywords = {concentric tube robots, medical robots and systems, mechanism design of manipulators, continuum robots, Cable-driven robots, Monte Carlo methods, Electron tubes, Cable-driven mechanisms, Power cables, Monte Carlo method, Medical robotics, surgery, Service robots, Kinematics, Minimally invasive surgery, concentric tube mechanisms, dexterity analysis, dexterity distribution, directional dexterity indices, minimally invasive surgeries, orientability concept},
	pages = {514--521},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\6MWAHC3B\\Wu 等. - 2017 - Dexterity Analysis of Three 6-DOF Continuum Robots.pdf:application/pdf}
