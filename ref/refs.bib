  keywords = {Cable drive,conduit contact interface,elasticity,elastic manipulator,flexible manipulator,Kinematics,linear system,Manipulator dynamics,manipulator kinematics,mechanics modeling,Medical robotics,Nonlinear control systems,nonlinear tendon loads,optimal feasible controller,redundant tendons,Robotic surgery,slack tendon,snake robot,statics,tendon drive,tendon-driven continuum manipulators},
  pages = {1262--1273},
