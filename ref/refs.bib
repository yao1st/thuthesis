  month = feb,
  year = {2003},
  pages = {45--63},
  file = {Full Text PDF:C\:\\Users\\Yao\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dafbbgmf.default\\zotero\\storage\\T2A4EAN8\\Hannan 和 Walker - 2003 - Kinematics and the Implementation of an Elephant's.pdf:application/pdf;Snapshot:C\:\\Users\\Yao\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dafbbgmf.default\\zotero\\storage\\QC4ZSBMA\\abstract\;jsessionid=2D40472959F513FE3488965FB7C231F9.html:text/html}
