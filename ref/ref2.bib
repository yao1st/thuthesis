	pages = {465--475},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\6TFSSDT3\\7574345.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\CJHJFN8P\\Gao 等. - 2017 - Mechanical Model of Dexterous Continuum Manipulato.pdf:application/pdf}
