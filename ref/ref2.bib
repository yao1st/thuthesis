	pages = {917--923},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Yao\\Zotero\\storage\\U4F4EBDD\\7487223.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\Zotero\\storage\\ZFRGVIG2\\Neumann 和 Burgner-Kahrs - 2016 - Considerations for follow-the-leader motion of ext.pdf:application/pdf}
