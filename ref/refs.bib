  year = {2016},
  keywords = {Artificial neural networks,biocybernetics,CBHA manipulator kinematics,compact bionic handling assistant,continuum manipulators,continuum robots,control,Electron tubes,IKE,intervertebra,inverse kinematic equations,Joints,Kinematics,manipulator kinematics,Manipulators,Mathematical model,mechanical engineering computing,modeling,neural nets,Neural networks,parallel rigid manipulators,soft manipulator,universal-prismatic-spherical joints,vertebrae},
  pages = {1326--1335},
  file = {IEEE Xplore Full Text PDF:C\:\\Users\\Yao\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dafbbgmf.default\\zotero\\storage\\E6B4UV3I\\Lakhal 等. - 2016 - Hybrid Approach for Modeling and Solving of Kinema.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\Yao\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dafbbgmf.default\\zotero\\storage\\8W2HQ5B6\\7296659.html:text/html}
